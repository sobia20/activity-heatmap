#!/home/sobiaa/chEMBL-Heatmap/backend/backend-env/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'chembl-webresource-client==0.10.0','console_scripts','chembl_act'
__requires__ = 'chembl-webresource-client==0.10.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('chembl-webresource-client==0.10.0', 'console_scripts', 'chembl_act')()
    )
